      <ImageView fitHeight="140.0" fitWidth="145.0" layoutX="346.0" layoutY="452.0" onMouseClicked="#star" onMouseEntered="#glowImage" onMouseExited="#stopGlowing" pickOnBounds="true" preserveRatio="true">
