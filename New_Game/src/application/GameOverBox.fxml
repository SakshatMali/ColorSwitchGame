            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label fx:id="curr" layoutX="349.0" layoutY="244.0" prefHeight="54.0" prefWidth="127.0" style="-fx-background-color: white;" />
      <Label fx:id="max" layoutX="349.0" layoutY="314.0" prefHeight="54.0" prefWidth="127.0" style="-fx-background-color: white;" />
      <Label fx:id="total" layoutX="348.0" layoutY="383.0" prefHeight="54.0" prefWidth="127.0" style="-fx-background-color: white;" />
   </children>
</AnchorPane>
