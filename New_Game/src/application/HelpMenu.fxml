<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HelpMenuController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Colour%20Images/HelpMenu.png" />
         </image>
      </ImageView>
      <Pane layoutX="466.0" layoutY="23.0" prefHeight="120.0" prefWidth="120.0">
